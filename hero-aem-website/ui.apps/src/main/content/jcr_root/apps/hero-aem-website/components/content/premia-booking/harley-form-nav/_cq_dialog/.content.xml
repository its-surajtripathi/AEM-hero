<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Harley Devidson Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <harleylogo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Choose Harley icon Image"
                                                name="./harleylogo"
                                                rootPath="/content/dam/hero-aem-website"/>
                                            <active
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./active"
                                                text="Make the ProgressBar active"
                                                uncheckedValue="false"
                                                value="true"/>
                                            <headermulti
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Hero Details"
                                                fieldLabel="Hero Details"
                                                required="{Boolean}false">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    emptyText="Hero Carousel Details"
                                                    name="./premiaheader">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <menu
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Enter menu"
                                                            name="./menu"/>
                                                        <iconurl
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Enter Icon URL"
                                                            name="./iconurl"
                                                            rootPath="/content/hero-aem-website"/>
                                                    </items>
                                                </field>
                                            </headermulti>
                                            <mobiconpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Choose phone icon Image"
                                                name="./mobiconpath"
                                                rootPath="/content/dam/hero-aem-website"/>
                                            <number
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Phone number"
                                                name="./number"/>
                                            <numbertip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Tool Tip for Phone number"
                                                name="./numbertip"/>
                                            <mailiconpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Choose Mail icon Image"
                                                name="./mailiconpath"
                                                rootPath="/content/dam/hero-aem-website"/>
                                            <mail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Mail ID"
                                                name="./mail"/>
                                            <mailtip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Tool Tip for Mail ID"
                                                name="./mailtip"/>
                                            <proficonpath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Choose Profile icon Image"
                                                name="./proficonpath"
                                                rootPath="/content/dam/hero-aem-website"/>
                                            <proficonurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Enter Profile URL"
                                                name="./proficonurl"
                                                rootPath="/content/hero-aem-website"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
