<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dealers Portal Login Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <loginHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Login Heading "
                                                name="./loginHeading"
                                                required="{Boolean}true"/>
                                            <dealerCodeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Dealer Code Label"
                                                name="./dealerCodeLabel"
                                                required="{Boolean}true"/>
                                            <dealerCodePlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Dealer Code Placeholder"
                                                name="./dealerCodePlaceholder"
                                                required="{Boolean}true"/>
                                            <mobileLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Mobile no Label"
                                                name="./mobileLabel"
                                                required="{Boolean}true"/>
                                            <mobilePlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Mobile no Placeholder"
                                                name="./mobilePlaceholder"
                                                required="{Boolean}true"/>
                                            <mobileRequired
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Required message"
                                                name="./mobileRequired"
                                                required="{Boolean}true"/>
                                            <mobileValidation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Validation message"
                                                name="./mobileValidation"
                                                required="{Boolean}true"/>
                                            <mobilenotFound
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Number Not Found message"
                                                name="./mobilenotFound"
                                                required="{Boolean}true"/>
                                            <otpLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter OTP Label"
                                                name="./otpLabel"
                                                required="{Boolean}true"/>
                                            <otpPlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter OTP Placeholder"
                                                name="./otpPlaceholder"
                                                required="{Boolean}true"/>
                                            <otpRequired
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="OTP Required message"
                                                name="./otpRequired"
                                                required="{Boolean}true"/>
                                            <otpValidation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="OTP Validation message"
                                                name="./otpValidation"
                                                required="{Boolean}true"/>
                                            <incorrectOtp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Incorrect Otp message"
                                                name="./incorrectOtp"
                                                required="{Boolean}true"/>
                                            <sendOtp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Send OTP label"
                                                name="./sendOtpLabel"
                                                required="{Boolean}true"/>
                                            <resendOtp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Resend OTP label"
                                                name="./resendOtpLabel"
                                                required="{Boolean}true"/>
                                            <serverdownMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Server Down message"
                                                name="./serverdownMsg"
                                                required="{Boolean}true"/>
                                            <loginLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Enter Login Submit Label"
                                                name="./loginLabel"
                                                required="{Boolean}true"/>
                                            <afterloginpage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                fieldLabel="Enter page path after login"
                                                name="./afterlogin"
                                                required="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
