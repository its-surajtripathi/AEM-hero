<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Reports Toolkit"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <container
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <toolkit
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <multireports
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldLabel="click on add button to add Toolkit Items">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./toolkititems">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <title
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                            fieldLabel="Enter ToolKit Item Heading"
                                                                            name="./toolkitheading"
                                                                            useFixedInlineToolbar="{Boolean}true">
                                                                            <rtePlugins
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceSuperType="/apps/hero-aem-website/components/content/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/rtePlugins"/>
                                                                            <uiSettings
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceSuperType="/apps/hero-aem-website/components/content/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/uiSettings"/>
                                                                        </title>
                                                                        <ctalabel
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldDescription="Please add the CTA Label"
                                                                            fieldLabel="Enter CTA Label"
                                                                            name="./viewctalabel"/>
                                                                        <ctaurl
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldLabel="CTA URL"
                                                                            name="./viewctaurl"
                                                                            rootPath="/content/hero-aem-website"/>
                                                                        <reports
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <multireports
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                                    composite="{Boolean}true"
                                                                                    fieldLabel="click on add button to add Reports">
                                                                                    <field
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                                        name="./toolkitreports">
                                                                                        <items jcr:primaryType="nt:unstructured">
                                                                                            <reportlabel
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                                                fieldLabel="Enter Report Label"
                                                                                                name="./inreportlabel"
                                                                                                useFixedInlineToolbar="{Boolean}true">
                                                                                                <rtePlugins
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceSuperType="/apps/hero-aem-website/components/content/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/rtePlugins"/>
                                                                                                <uiSettings
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceSuperType="/apps/hero-aem-website/components/content/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/uiSettings"/>
                                                                                            </reportlabel>
                                                                                            <reportpath
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                                fieldLabel="Report path"
                                                                                                name="./inreporturl"
                                                                                                rootPath="/content/dam/hero-aem-website"/>
                                                                                            <icon
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                                fieldLabel="Download Icon Path"
                                                                                                name="./indownloadicon"
                                                                                                rootPath="/content/dam/hero-aem-website"/>
                                                                                        </items>
                                                                                    </field>
                                                                                </multireports>
                                                                            </items>
                                                                        </reports>
                                                                    </items>
                                                                </field>
                                                            </multireports>
                                                        </items>
                                                    </toolkit>
                                                </items>
                                            </container>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </container>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
