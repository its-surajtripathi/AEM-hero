<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Good Life Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <service
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <servicefields
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Service Details"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <serviceLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Service Label"
                                                                name="./serviceLabel"
                                                                required="{Boolean}true"/>
                                                            <serviceIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Service Icon"
                                                                name="./serviceicon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <timeLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Time Label"
                                                                name="./timeLabel"
                                                                required="{Boolean}true"/>
                                                            <odometerLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Odometer Label"
                                                                name="./odometerLabel"
                                                                required="{Boolean}true"/>
                                                            <odometerHelpText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                fieldLabel="Odometer Help Text"
                                                                name="./odometerHelpText"
                                                                required="{Boolean}true"/>
															<odometerHelpIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Help Icon"
                                                                name="./odometerHelpIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <servicetypeLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Service Type Label"
                                                                name="./servicetypeLabel"
                                                                required="{Boolean}true"/>
                                                            <freeserviceLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Free Service Label"
                                                                name="./freeserviceLabel"
                                                                required="{Boolean}true"/>
                                                            <freeserviceIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Free Service Icon"
                                                                name="./freeserviceIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <paidserviceLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Paid Service Label"
                                                                name="./paidserviceLabel"
                                                                required="{Boolean}true"/>
                                                            <paidserviceIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Paid Service Icon"
                                                                name="./paidserviceIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <joyrideLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Joyride Label"
                                                                name="./joyrideLabel"
                                                                required="{Boolean}true"/>
                                                            <joyrideIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Joyride Icon"
                                                                name="./joyrideIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <serviceDone
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Service Done Label"
                                                                name="./serviceDone"
                                                                required="{Boolean}true"/>
                                                            <atLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter At Label"
                                                                name="./atLabel"
                                                                required="{Boolean}true"/>
                                                            <kmLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Km Label"
                                                                name="./kmLabel"
                                                                required="{Boolean}true"/>
                                                            <tickIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Green Tick Icon"
                                                                name="./tickIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <linegapIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Line Gap Icon"
                                                                name="./linegapIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <nextbuttonLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Next Button Label"
                                                                name="./nextbuttonLabel"
                                                                required="{Boolean}true"/>
                                                        </items>
                                                    </servicefields>
                                                </items>
                                            </service>
                                            <dealer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <dealerfields
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Dealer "
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <dealerLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Dealer Label"
                                                                name="./dealerLabel"
                                                                required="{Boolean}true"/>
                                                            <dealerIcon
                                                                jcr:primaryType="nt:unstructured"
