                                                            <statePlaceholder
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter State Placeholder"
                                                                name="./statePlaceholder"
                                                                required="{Boolean}true"/>
                                                            <statevalmsg
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter State Validation Message"
                                                                name="./statevalmsg"
                                                                required="{Boolean}true"/>
                                                            <cityLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter City Label"
                                                                name="./cityLabel"
                                                                required="{Boolean}true"/>
                                                            <cityPlaceholder
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter City Placeholder"
                                                                name="./cityPlaceholder"
                                                                required="{Boolean}true"/>
                                                            <cityvalmsg
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter City Validation Message"
                                                                name="./cityvalmsg"
                                                                required="{Boolean}true"/>
                                                            <orLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter or Label"
                                                                name="./orLabel"
                                                                required="{Boolean}true"/>
                                                            <locationLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Current Location Label"
                                                                name="./locationLabel"
                                                                required="{Boolean}true"/>
                                                            <locationIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Location Icon"
                                                                name="./locationIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <locationEnable
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Location Enable  Message"
                                                                name="./locationEnable"
                                                                required="{Boolean}true"/>
                                                            <nodealermsg
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter No Dealer Message"
                                                                name="./nodealermsg"
                                                                required="{Boolean}true"/>
                                                            <phoneIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Phone Icon"
                                                                name="./phoneIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <backbuttonLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Back Button Label"
                                                                name="./backbuttonLabel"
                                                                required="{Boolean}true"/>
                                                        </items>
                                                    </dealerfields>
                                                </items>
                                            </dealer>
                                            <selfjobcard
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <selfjobcardfields
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Self Job Card"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <selfjobcardLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Self Job Card Label"
                                                                name="./selfjobcardLabel"
                                                                required="{Boolean}true"/>
                                                            <selfjobcardDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Self Job Card Description"
                                                                name="./selfjobcardDescription"
                                                                required="{Boolean}true"/>
                                                            <selfjobcardIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Self Job Card Icon"
                                                                name="./selfjobcardIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <tyreandbrakeIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Tyre and Brake Icon"
                                                                name="./tyreandbrakeIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <tyreandbrakeLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Tyre and Brake Label"
                                                                name="./tyreandbrakeLabel"
                                                                required="{Boolean}true"/>
                                                            <fuelIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Fuel Icon"
                                                                name="./fuelIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <fuelLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Fuel Label"
                                                                name="./fuelLabel"
                                                                required="{Boolean}true"/>
                                                            <keyIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Key Icon"
                                                                name="./keyIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <keyLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Key Label"
                                                                name="./keyLabel"
                                                                required="{Boolean}true"/>
                                                            <otherissuesLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Other Issues Label"
                                                                name="./otherissuesLabel"
                                                                required="{Boolean}true"/>
                                                            <otherissuePlaceholder
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Other Issues Placeholder"
                                                                name="./otherissuesPlaceholder"
                                                                required="{Boolean}true"/>
                                                        </items>
                                                    </selfjobcardfields>
                                                </items>
                                            </selfjobcard>
                                            <summary
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <summaryfields
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Summary"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <summaryLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Summary Label"
                                                                name="./summaryLabel"
                                                                required="{Boolean}true"/>
                                                            <summaryIcon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Select Summary Icon"
                                                                name="./summaryIcon"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam/hero-aem-website"/>
                                                            <basicdetailsLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Basic Details Label"
                                                                name="./basicdetailsLabel"
                                                                required="{Boolean}true"/>
