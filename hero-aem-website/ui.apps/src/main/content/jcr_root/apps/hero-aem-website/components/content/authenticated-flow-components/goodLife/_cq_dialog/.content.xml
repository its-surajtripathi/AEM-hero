                                                            <servicetime
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Service Time Label"
                                                                name="./servicetime"
                                                                required="{Boolean}true"/>
                                                            <servicetype
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Service Type Label"
                                                                name="./servicetype"
                                                                required="{Boolean}true"/>
                                                            <selfjobcard
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Self Job Card Label"
                                                                name="./selfjobcard"
                                                                required="{Boolean}true"/>
                                                            <dealerLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Dealer  Label"
                                                                name="./dealer"
                                                                required="{Boolean}true"/>
                                                            <tyreandbrake
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Tyre and Brake Label"
                                                                name="./tyreandbrake"
                                                                required="{Boolean}true"/>
                                                            <fuel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
