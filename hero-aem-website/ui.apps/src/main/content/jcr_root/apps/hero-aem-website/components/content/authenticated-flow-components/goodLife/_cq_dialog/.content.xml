                                                                required="{Boolean}true"/>
                                                            <changeLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Change Label"
                                                                name="./changeLabel"
                                                                required="{Boolean}true"/>
                                                            <booknowLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Enter Book Now Label"
                                                                name="./booknowLabel"
                                                                required="{Boolean}true"/>
                                                        </items>
                                                    </summaryfields>
                                                </items>
